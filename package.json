{
    "name": "@daveyplate/better-auth-ui-vue",
    "homepage": "https://better-auth-ui.com",
    "version": "2.0.15",
    "description": "Plug & play Vue 3 components for better-auth",
    "scripts": {
        "build": "vue-tsc --noEmit && vite build",
        "dev": "vite",
        "prepublishOnly": "rm -rf dist && turbo build"
    },
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        },
        "./server": {
            "import": {
                "types": "./dist/server.d.ts",
                "default": "./dist/server.js"
            },
            "require": {
                "types": "./dist/server.d.cts",
                "default": "./dist/server.cjs"
            }
        },
        "./tanstack": {
            "import": {
                "types": "./dist/tanstack.d.ts",
                "default": "./dist/tanstack.js"
            },
            "require": {
                "types": "./dist/tanstack.d.cts",
                "default": "./dist/tanstack.cjs"
            }
        },
        "./instantdb": {
            "import": {
                "types": "./dist/instantdb.d.ts",
                "default": "./dist/instantdb.js"
            },
            "require": {
                "types": "./dist/instantdb.d.cts",
                "default": "./dist/instantdb.cjs"
            }
        },
        "./triplit": {
            "import": {
                "types": "./dist/triplit.d.ts",
                "default": "./dist/triplit.js"
            },
            "require": {
                "types": "./dist/triplit.d.cts",
                "default": "./dist/triplit.cjs"
            }
        }
    },
    "files": [
        "src",
        "dist"
    ],
    "keywords": [
        "typescript",
        "vue",
        "vue3",
        "better-auth",
        "shadcn",
        "nuxt"
    ],
    "author": "daveycodez",
    "license": "MIT",
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "@tailwindcss/postcss": "^4.1.11",
        "@tanstack/vue-query": "^5.80.7",
        "@types/node": "^24.0.1",
        "@vitejs/plugin-vue": "^5.2.2",
        "@vue/compiler-sfc": "^3.4.38",
        "@vue/tsconfig": "^0.7.0",
        "autoprefixer": "^10.4.21",
        "better-auth": "^1.2.9",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.11",
        "tailwindcss-animate": "^1.0.7",
        "turbo": "^2.5.4",
        "typescript": "^5.8.3",
        "unplugin-vue-components": "^0.28.0",
        "vite": "^6.0.6",
        "vite-plugin-dts": "^4.3.0",
        "vue": "^3.4.38",
        "vue-tsc": "^2.2.0"
    },
    "peerDependencies": {
        "@daveyplate/better-auth-tanstack": "^1.3.6",
        "@tanstack/vue-query": ">=5.66.0",
        "@triplit/client": ">=1.0.0",
        "@vee-validate/zod": ">=4.15.0",
        "@vueuse/core": ">=12.0.0",
        "better-auth": "^1.2.9",
        "class-variance-authority": ">=0.7.0",
        "clsx": ">=2.1.0",
        "lucide-vue-next": ">=0.469.0",
        "tailwind-merge": ">=2.6.0",
        "tailwindcss": ">=3.0.0",
        "vee-validate": ">=4.15.0",
        "vue": "^3.5.0",
        "vue-router": "^4.5.0",
        "vue-sonner": ">=1.2.0",
        "zod": ">=3.24.0"
    },
    "dependencies": {
        "@better-fetch/fetch": "^1.1.18",
        "@hcaptcha/vue-hcaptcha": "^1.3.0",
        "@vueuse/core": "^12.5.0",
        "js-sha256": "^0.11.1",
        "lucide-vue-next": "^0.515.0",
        "qrcode.vue": "^3.6.0",
        "radix-vue": "^1.9.17",
        "ua-parser-js": "^2.0.3",
        "v-clipboard": "3.0.0-next.1",
        "vaul-vue": "^0.4.1",
        "vue-email": "^0.8.11",
        "vue-recaptcha": "^3.0.0-alpha.6",
        "vue-recaptcha-v3": "^2.0.1",
        "vue-turnstile": "^1.0.11"
    },
    "pnpm": {
        "overrides": {}
    },
    "packageManager": "pnpm@10.12.1"
}